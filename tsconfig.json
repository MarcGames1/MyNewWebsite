{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/contact.js", "components/FormularContact/FormularContact.jsx", "components/Despre Mine/AccordionForm.jsx", "pages/blog/index.tsx", "scripts/FilterPosts.js", "pages/tutoriale/[slug].js"],
  "exclude": ["node_modules"]
}
